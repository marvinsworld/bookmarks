{
  "manifest_version": 2,
  "name": "Eason's BookMark",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",
  "background": {
    "scripts": ["jquery.min.js","json2.js","syncBookmarks.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
  	"bookmarks",
    "activeTab",
    "http://*/*",
    "https://*/*"
  ]
}